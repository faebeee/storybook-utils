{
  "name": "storybook-axios",
  "type": "module",
  "description": "Axios inspection for storybook",
  "version": "2.2.0",
  "scripts": {
    "lint": "eslint ./src",
    "format": "eslint ./src --fix",
    "build": "npx tsc",
    "dev": "npx tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "storybook",
    "axios",
    "network",
    "documentation",
    "addon",
    "storybook-addons"
  ],
  "exports": {
    "./manager": "./lib/manager.ts",
    "./decorator": "./lib/decorator.ts"
  },
  "author": "faebeee",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/faebeee/storybook-axios"
  },
  "peerDependencies": {
    "axios": "^1.12.2",
    "react": ">=18",
    "react-dom": ">=18",
    "storybook": "^9.1.16"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.16",
    "axios-mock-adapter": "^2.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "axios": "^1.12.2",
    "@repo/typescript-config": "*"
  },
  "storybook": {
    "displayName": "storybook-axios",
    "unsupportedFrameworks": [],
    "supportedFrameworks": [
      "React",
      "Vue",
      "Angular"
    ]
  }
}
